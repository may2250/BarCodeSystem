<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CusLongInput">    <!-- *记住* declare-styleable的name就是自定义控件的名字 -->
        <attr name="IconDrawable" format="reference|integer" />
        <attr name="hint" format="reference|string" />
        <attr name="text" format="reference|string" />
        <attr name="textSize" format="reference|dimension" />
